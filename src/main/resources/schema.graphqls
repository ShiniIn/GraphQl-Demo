
type Requirement{
id:Long
description: String
name:String
}

type TestCase{
id:Long
description: String
name:String
}

type Query {
  findAllTestcases: [TestCase]
  findAllRequirement:[Requirement]
  countTestcase:Long
  countRequirement:Long

}

type Mutation{
newRequirement(id:Long,name:String,description:String): Requirement
newTestCase(id:Long,name:String,description:String): TestCase
}

schema {
  query: Query
  mutation: Mutation
}